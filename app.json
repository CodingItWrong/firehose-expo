{
  "expo": {
    "name": "Firehose",
    "description": "A simple link-saving app focused on reading.",
    "githubUrl": "https://github.com/CodingItWrong/firehose-expo",
    "slug": "firehose",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#B71C1C"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "buildNumber": "3",
      "supportsTablet": true,
      "bundleIdentifier": "com.codingitwrong.firehose",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "name": "Firehose",
      "themeColor": "#B71C1C",
      "favicon": "./assets/favicon.png"
    }
  }
}
